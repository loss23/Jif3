pro:NewButton("Anti-Admins kick", "If any Admin/Owner joins, you get kicked", function()
   
                game.StarterGui:SetCore("SendNotification", {
        Title = "‚ñÄ‚ñÑ„ÄéR„Äè„Äé1„Äè‚ñÑ‚ñÄ";
        Text = "Anti-Admins kick toggled: \n true";
        Icon = "rbxassetid://10223592622"; 
        Duration = 15;
    })
   
    local admins = {"nickaids", "FireIsLeBigPro", "PANSMIETANKAOK", "sparkey588225", "C10UDZIES", "lighningbow", "AmInsomniac", "Lawrence4310", "JerryVn2005", "Lianos", "mr_angered", "0000_20202", "sytherplay", "ZuiMas", "Kralmice", "Lxqryz", "CallMeShadeGamer", "Ciberthor2", "NokiSou", "marix8275", "AssonTR2", "AdventureGirlFion"}

    for _, v in pairs(game:GetService("Players"):GetChildren()) do
        if table.find(admins, tostring(v.Name)) then
            game:GetService("Players").LocalPlayer:Kick("\nAnti-Admin: Admin In The Server\n\nName: "..tostring(v.Name) .." \nDisplay Name: "..tostring(v.DisplayName) .."\n")
        end
    end
    
    game:GetService("Players").PlayerAdded:Connect(function(plr)
        if table.find(admins, tostring(plr.Name)) then
            game:GetService("Players").LocalPlayer:Kick("\nAnti-Admin: Admin Joined\n\nName: "..tostring(plr.Name) .." \nDisplay Name: "..tostring(plr.DisplayName) .."\n")
        end
    end)
end)

