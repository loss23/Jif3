local Window = Library.CreateLib("Aurora X" ..ver2, "_G.Settings.Theme")

local PP = Window:NewTab("Player")
local EN = Window:NewTab("Enemies")
local EF = Window:NewTab("Effects")
local WO = Window:NewTab("World")
local SO = Window:NewTab("Stand")
local SP = Window:NewTab("Special")
local OW = Window:NewTab("Owner")


local player = PP:NewSection("Player")
