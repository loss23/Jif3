--Player

local value = false
player:NewToggle("Godmode", "MAKES YOU A GOD", function(s)
  value = s
    if s then
        local d
        d = game:service('RunService').Stepped:connect(function()
            if not value then
                return d:Disconnect()
            end
 A_1 = CFrame.new(-56144, 6359, -39824)
 A_2 = 100
 A_3 = "rbxassetid://1837879082"
 A_4 = 0
 A_5 = 0
game:GetService("ReplicatedStorage").Heal2:FireServer(A_1, A_2, A_3, A_4, A_5)
end)
end
end)


