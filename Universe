--Special
spec:NewTextBox("Explode User", "Kills ANYONE no matter what", function(user)
    user = string.lower(user)
    for _, v in pairs(game.Players:GetPlayers()) do
        if string.find(string.lower(v.Name), user) or string.find(string.lower(v.DisplayName), user)  then
            targetted = tostring(v.Name)
        end
    end


        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[targetted].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,0)
        wait(0.5)
         game.Players.LocalPlayer.Character.Head:Remove()
        game:GetService("ReplicatedStorage").ExplosionUltCookie:FireServer()
     game.Players.LocalPlayer.Character.Head:Remove()
end) 

spec:NewButton("BOMB", "Kills ANYONE no matter what", function()
game:GetService("ReplicatedStorage").ExplosionUltCookie:FireServer()
end)


spec:NewButton("Rainbow jump", "Does a funny animation", function()
while wait do
wait()
A_1 = BrickColor.new("Bright red")
game:GetService("ReplicatedStorage").Jump:FireServer(A_1)
wait()
A_1 = BrickColor.new("Bright yellow")
game:GetService("ReplicatedStorage").Jump:FireServer(A_1)
wait()
A_1 = BrickColor.new("Bright blue")
game:GetService("ReplicatedStorage").Jump:FireServer(A_1)
end
end)




